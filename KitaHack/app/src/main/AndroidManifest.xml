<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.KitaHack"
        tools:targetApi="31">
        <activity
            android:name=".GeminiChatScreen"
            android:exported="false"
            android:label="@string/title_activity_gemini_chat_screen"
            android:theme="@style/Theme.KitaHack" />
        <activity
            android:name=".LoginFirebase"
            android:exported="false"
            android:label="@string/title_activity_login_firebase"
            android:theme="@style/Theme.KitaHack" />
        <activity
            android:name=".Discover"
            android:exported="false"
            android:label="@string/title_activity_discover"
            android:theme="@style/Theme.KitaHack" />
        <activity
            android:name=".Profile"
            android:exported="false"
            android:label="@string/title_activity_profile"
            android:theme="@style/Theme.KitaHack" />
        <activity
            android:name=".Setting"
            android:exported="false"
            android:label="@string/title_activity_setting"
            android:theme="@style/Theme.KitaHack" />
        <activity
            android:name=".HomePage"
            android:exported="false"
            android:label="@string/title_activity_home_page"
            android:theme="@style/Theme.KitaHack" />
        <activity
            android:name=".SignUpPage"
            android:exported="false"
            android:label="@string/title_activity_sign_up_page"
            android:theme="@style/Theme.KitaHack" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.KitaHack">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>